# The URL of your main n8n chat webhook trigger
VITE_N8N_WEBHOOK_URL="https://your-n8n.instance.com/webhook/chat"

# The URL of your n8n webhook for retrieving chat history
VITE_N8N_GET_HISTORY_URL="https://your-n8n.instance.com/webhook/get-history"

# Set to "true" to show the sample chat data on startup instead of fetching history
VITE_SHOW_SAMPLE_CHAT="false"
